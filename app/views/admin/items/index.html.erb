<div class= "container">
 <div class= "row">
  <h1>商品一覧</h1>
   <table class= "table">
     <tr>
       <th>商品ID</th>
       <th>商品名</th>
       <th>税抜価格</th>
       <th>販売ステータス</th>
     </tr>
     <% @items.each do |item| %>
      <tr>
       <td><%= item.item_id %></td>
       <td><%= link_to item.name, admin_items_path(@item) %></td>
       <td><%= item.price %></td>
       <td><%= item.is_active_i18n %></td>
      </tr>
     <% end %>
   </table>
 </div>
</div>